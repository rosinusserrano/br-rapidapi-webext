{
    "manifest_version": 2,
    "name": "BR RapidApi Injection",
    "version": "1.0",
    "description": "Replaces text with result from bionic reading rapid api",
    "permissions": [
        "activeTab",
        "http://*/*",
        "https://*/*",
        "storage"
    ],
    "options_ui": {
        "page": "options.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}